#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jrogers/code/github/kids-multi-wiki-chat/mcp-servers/lego/node_modules/.pnpm/@professional-wiki+mediawiki-mcp-server@0.1.1/node_modules/@professional-wiki/mediawiki-mcp-server/dist/node_modules:/Users/jrogers/code/github/kids-multi-wiki-chat/mcp-servers/lego/node_modules/.pnpm/@professional-wiki+mediawiki-mcp-server@0.1.1/node_modules/@professional-wiki/mediawiki-mcp-server/node_modules:/Users/jrogers/code/github/kids-multi-wiki-chat/mcp-servers/lego/node_modules/.pnpm/@professional-wiki+mediawiki-mcp-server@0.1.1/node_modules/@professional-wiki/node_modules:/Users/jrogers/code/github/kids-multi-wiki-chat/mcp-servers/lego/node_modules/.pnpm/@professional-wiki+mediawiki-mcp-server@0.1.1/node_modules:/Users/jrogers/code/github/kids-multi-wiki-chat/mcp-servers/lego/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jrogers/code/github/kids-multi-wiki-chat/mcp-servers/lego/node_modules/.pnpm/@professional-wiki+mediawiki-mcp-server@0.1.1/node_modules/@professional-wiki/mediawiki-mcp-server/dist/node_modules:/Users/jrogers/code/github/kids-multi-wiki-chat/mcp-servers/lego/node_modules/.pnpm/@professional-wiki+mediawiki-mcp-server@0.1.1/node_modules/@professional-wiki/mediawiki-mcp-server/node_modules:/Users/jrogers/code/github/kids-multi-wiki-chat/mcp-servers/lego/node_modules/.pnpm/@professional-wiki+mediawiki-mcp-server@0.1.1/node_modules/@professional-wiki/node_modules:/Users/jrogers/code/github/kids-multi-wiki-chat/mcp-servers/lego/node_modules/.pnpm/@professional-wiki+mediawiki-mcp-server@0.1.1/node_modules:/Users/jrogers/code/github/kids-multi-wiki-chat/mcp-servers/lego/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/index.js" "$@"
else
  exec node  "$basedir/../../dist/index.js" "$@"
fi
